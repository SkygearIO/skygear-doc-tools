#!/usr/bin/env python
# vim: set ft=python:
import click
from doc_tools import *

@click.command()
@click.option('--pwd', help='project directory', required=True)
@click.option('--prefix', help='github prefix', required=True)
def generate_ios_doc(pwd, prefix):
    os.chdir(pwd)
    run_command('ios', pwd, 'jazzy --github-file-prefix %s' % (prefix), False)

if __name__ == '__main__':
    generate_ios_doc()
